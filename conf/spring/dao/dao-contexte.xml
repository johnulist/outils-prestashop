<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="productDao" class="fr.upyourbizz.web.persistence.ProductDao">
		<property name="ds" ref="dataSource" />
	</bean>
	<bean id="produitReferenceDao" class="fr.upyourbizz.web.persistence.ProduitReferenceDao">
		<property name="ds" ref="dataSource" />
	</bean>
	<bean id="utilisateurDao" class="fr.upyourbizz.web.persistence.UtilisateurDao">
		<property name="ds" ref="dataSource" />
	</bean>
	<bean id="clientDao" class="fr.upyourbizz.web.persistence.ClientDao">
		<property name="ds" ref="dataSource" />
	</bean>
</beans>